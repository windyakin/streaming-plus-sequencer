{
  "name": "streaming-plus-sequencer",
  "version": "1.3.2",
  "private": true,
  "scripts": {
    "build": "ts-node -T build.ts",
    "build:watch": "ts-node -T build.ts --watch",
    "lint": "eslint",
    "dev:firefox": "run-p build:watch ext:firefox",
    "dev:chrome": "run-p build:watch ext:chrome",
    "ext:firefox": "web-ext run",
    "ext:chrome": "web-ext run -t chromium",
    "package": "web-ext build"
  },
  "devDependencies": {
    "@types/mustache": "4.2.6",
    "@types/node": "22.19.7",
    "@types/webextension-polyfill": "0.12.4",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "esbuild": "0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "prettier": "3.8.1",
    "npm-run-all2": "^8.0.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "web-ext": "9.2.0",
    "webextension-polyfill": "0.12.0"
  }
}
